<odoo>
    <data>
        <record model="ir.ui.view" id="profesores.estudiantes_list">
            <field name="name">Datos del Profesorado</field>
            <field name="model">profesores.estudiantes</field>
            <field name="arch" type="xml">
                <tree>
                    <group>
                        <field name="nombre"/>
                        <field name="apellidos"/>
                        <field name="activo"/>
                        <field name="asignatura"/>
                        <field name="descripcion"/>
                        <field name="fecha_Inscripcion"/>
                        <field name="estudiantes_id"/>
                    </group>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="profesores.estudiantes_form">
            <field name="name">Datos del Profesorado</field>
            <field name="model">profesores.estudiantes</field>
            <field name="arch" type="xml">
                <form>
                    <field name="nombre"/>
                    <field name="apellidos"/>
                    <field name="activo"/>
                    <field name="asignatura"/>
                    <field name="descripcion"/>
                    <field name="fecha_Inscripcion"/>
                    <group>
                        <field name="estudiantes_id"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="profesores.action_window">
            <field name="name">Listado de profesores</field>
            <field name="res_model">profesores.estudiantes</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Listado Profesores" id="estudiantes.menu_1_list_profesores" parent="estudiantes.menu_estudiantes" action="profesores.action_window"/>
    </data>
</odoo>
